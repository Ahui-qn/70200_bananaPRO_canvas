# Nano Banana AI API 配置
# 请将 YOUR_API_KEY_HERE 替换为你的实际 API 密钥
VITE_API_KEY=your_api_key_here

# 可选：API 基础地址（默认使用国内直连）
VITE_API_BASE_URL=https://grsai.dakka.com.cn/v1/draw

# 阿里云数据库配置
# 请根据你的实际数据库信息进行配置
VITE_DB_HOST=your-rds-host.mysql.rds.aliyuncs.com
VITE_DB_PORT=3306
VITE_DB_DATABASE=nano_banana_db
VITE_DB_USERNAME=your_username
VITE_DB_PASSWORD=your_password
VITE_DB_SSL=true

# 数据加密密钥（请使用强密钥并妥善保管）
# 生成方法：openssl rand -hex 32
VITE_ENCRYPTION_KEY=your-32-character-encryption-key-here

# 阿里云函数计算配置（可选）
VITE_FC_ENDPOINT=https://your-account-id.cn-hangzhou.fc.aliyuncs.com
VITE_FC_ACCESS_KEY_ID=your_access_key_id
VITE_FC_ACCESS_KEY_SECRET=your_access_key_secret