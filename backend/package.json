{
  "name": "nano-banana-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest --run",
    "user:create": "tsx src/scripts/manage-users.ts create",
    "user:list": "tsx src/scripts/manage-users.ts list",
    "user:disable": "tsx src/scripts/manage-users.ts disable",
    "user:enable": "tsx src/scripts/manage-users.ts enable",
    "user:set-role": "tsx src/scripts/manage-users.ts set-role",
    "update:dimensions": "tsx src/scripts/update-image-dimensions.ts",
    "cleanup:all": "tsx src/scripts/cleanup-all-data.ts --force",
    "cleanup:database": "tsx src/scripts/cleanup-database-only.ts",
    "cleanup:oss-orphans": "tsx src/scripts/cleanup-orphaned-oss-files.ts",
    "test:oss-permissions": "tsx src/scripts/test-oss-permissions.ts",
    "check:db": "tsx src/scripts/check-database-status.ts"
  },
  "dependencies": {
    "ali-oss": "^6.23.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.3",
    "mysql2": "^3.16.0",
    "sharp": "^0.34.5",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.16.13",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.14.0",
    "@types/uuid": "^10.0.0",
    "fast-check": "^4.5.3",
    "tsx": "^4.19.2",
    "typescript": "~5.8.2",
    "vitest": "^4.0.16"
  }
}
